FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 126.9;
    }
    walls
    {
        type            fixedValue;
        value           uniform 298;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 298;
    }
    faceplate
    {
        type            zeroGradient;
    }
}


